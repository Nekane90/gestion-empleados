	<queryString language="SQL">
		<![CDATA[SELECT 
    e.nombre,          
    f.fecha,           
    f.horaEntrada,
    f.horaSalida,
    f.tipoTrabajo,
    CASE 
        WHEN f.horaSalida < f.horaEntrada  
        THEN TIMEDIFF(ADDTIME(f.horaSalida, '12:00:00'), f.horaEntrada)
        ELSE TIMEDIFF(f.horaSalida, f.horaEntrada)
    END AS duracion_fila,
    CASE 
        WHEN f.horaSalida < f.horaEntrada 
        THEN TIME_TO_SEC(TIMEDIFF(ADDTIME(f.horaSalida, '12:00:00'), f.horaEntrada))
        ELSE TIME_TO_SEC(TIMEDIFF(f.horaSalida, f.horaEntrada))
    END AS segundos_valor
FROM fichajes f
INNER JOIN empleados e ON f.idEmpleado = e.idEmpleado  
WHERE f.idEmpleado = $P{p_idSeleccionado}
  AND YEAR(f.fecha) = $P{p_anioSeleccionado}
ORDER BY f.fecha ASC]]>
	</queryString>
