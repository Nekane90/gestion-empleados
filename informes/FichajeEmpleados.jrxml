	<queryString language="SQL">
		<![CDATA[SELECT 
    e.nombre,          
    f.fecha,           
    f.horaEntrada,
    f.horaSalida,
    f.tipoTrabajo,

    TIMEDIFF(
        CASE WHEN f.horaSalida < f.horaEntrada 
             THEN ADDTIME(CAST(f.horaSalida AS TIME), '12:00:00') 
             ELSE f.horaSalida 
        END, 
        f.horaEntrada
    ) AS duracion_fila,
 
    CAST(
        TIME_TO_SEC(
            TIMEDIFF(
                CASE WHEN f.horaSalida < f.horaEntrada 
                     THEN ADDTIME(CAST(f.horaSalida AS TIME), '12:00:00') 
                     ELSE f.horaSalida 
                END, 
                f.horaEntrada
            )
        ) AS SIGNED
    ) AS segundosvalor
FROM fichajes f
INNER JOIN empleados e ON f.idEmpleado = e.idEmpleado  
WHERE f.idEmpleado = $P{p_idSeleccionado}
  AND YEAR(f.fecha) = $P{p_anioSeleccionado}
ORDER BY f.fecha ASC]]>
	</queryString>
